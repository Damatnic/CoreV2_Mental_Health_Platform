# Crisis Services API Keys
VITE_988_API_ENDPOINT=https://api.988lifeline.org/v2
VITE_988_API_KEY=your_988_api_key_here
VITE_CRISIS_TEXT_API_KEY=your_crisis_text_api_key_here
VITE_RAPIDSOS_API_KEY=your_rapidsos_api_key_here

# Google Services (for location and hospital finding)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
VITE_GOOGLE_GEOCODING_API_KEY=your_google_geocoding_api_key_here

# Emergency Services Integration
VITE_EMERGENCY_DISPATCH_URL=https://api.911.gov/emergency
VITE_EMERGENCY_WEBSOCKET_URL=wss://dispatch.911.gov/ws

# Crisis Center APIs
VITE_SAMHSA_API_KEY=your_samhsa_api_key_here
VITE_NAMI_API_KEY=your_nami_api_key_here

# WebRTC Configuration
VITE_WEBRTC_STUN_SERVER=stun:stun.988lifeline.org:3478
VITE_WEBRTC_TURN_SERVER=turn:turn.988lifeline.org:3478
VITE_WEBRTC_TURN_USERNAME=username
VITE_WEBRTC_TURN_PASSWORD=password

# Application URLs
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:3000

# Feature Flags
VITE_ENABLE_988_INTEGRATION=true
VITE_ENABLE_911_INTEGRATION=true
VITE_ENABLE_CRISIS_TEXT=true
VITE_ENABLE_WEBRTC_CALLING=true
VITE_ENABLE_LOCATION_SERVICES=true
VITE_AUTO_ESCALATION_ENABLED=true

# Safety Thresholds
VITE_CRISIS_AUTO_DIAL_THRESHOLD=0.85
VITE_EMERGENCY_ESCALATION_THRESHOLD=0.95
VITE_MONITORING_ALERT_THRESHOLD=0.7

# Consent Settings
VITE_REQUIRE_CONSENT_FOR_988=true
VITE_REQUIRE_CONSENT_FOR_911=false  # False for imminent danger
VITE_REQUIRE_CONSENT_FOR_LOCATION=true

# Fallback Services
VITE_FALLBACK_CRISIS_NUMBER=1-800-273-8255
VITE_FALLBACK_TEXT_NUMBER=741741
VITE_FALLBACK_EMERGENCY_NUMBER=911